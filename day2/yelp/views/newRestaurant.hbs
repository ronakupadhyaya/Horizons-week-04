<form method="POST" action="/restaurant/new" style="max-width: 300px; margin: auto; margin-top: 50px; border: 0.5px solid gray; padding: 30px 30px 30px 30px">
  <h3>Register</h3>
  <div class="form-group">
    <label>Restaurant Name</label>
    <input type="text" name="restaurantName" class="form-control">
  </div>
  <div class="form-group">
    <label>Category</label>
    <input type="text" name="category" class="form-control">
  </div>
  <div class="form-group">
    <label>Price</label>
    <input type="Number" name="price" class="form-control">
  </div>
  <div class="form-group">
    <label>Open time</label>
    <input type="time" name="openTime" class="form-control">
  </div>
  <div class="form-group">
    <label>Close time</label>
    <input type="time" name="closingTime" class="form-control">
  </div>
  <div class="form-group">
    <label>Address</label>
    <input type="String" name="address" class="form-control">
  </div>
  <div class="form-group">
    <button class="btn btn-success">Register</button>
  </div>
</form>
