<form method="POST" style="max-width: 300px; margin: auto; margin-top: 50px; border: 0.5px solid gray; padding: 30px 30px 30px 30px">
  <h3>Add a restaurant</h3>
  <div class="form-group">
    <label>Name</label>
    <input type="text" name="restaurant" class="form-control">
  </div>
  <div class="form-group">
    <label>Category</label>
    <input type="text" name="category" class="form-control">
  </div>
  <div class="form-group">
    <label>Relative Price</label>
    <input type="text" name="price" class="form-control">
  </div>
  <div class="form-group">
    <label>Address</label>
    <input type="text" name="address" class="form-control">
  </div>
  <div class="form-group">
    <label>Open Time</label>
    <input type="text" name="openTime" class="form-control">
    <label>Close Time</label>
    <input type="text" name="closeTime" class="form-control">
  </div>
  <div class="form-group">
    <button class="btn btn-success">Add Restaurant</button>
  </div>
</form>
<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAyDqrttUkSj4h3skkorapjgYWPy4_z58A&callback=initMap"
        type="text/javascript"></script>
