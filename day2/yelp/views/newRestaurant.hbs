<body>
  <div class="container">
    <h1>Add a Restaurant</h1>
        <div class="form-group">
          <label class="col-lg-4 control-label">Name</label>
          <input  name="name" placeholder="Restaurant Name" class="form-control"  type="text">
        </div>
        <label class="col-lg-4 control-label">Category</label>
        <form action="INSERT LINK" method="post">
          <input list="browsers" name="browser">
          <datalist id="browsers">
            <option value="Chinese">
            <option value="Indian">
            <option value="Japanese">
            <option value="Mexican">
            <option value="Italian">
          </datalist>
          <input type="submit">
        </form>
        <div class="form-group" name="price">
          <label class="col-lg-4 control-label">How Expensive Fham?</label>
          <input type="range" name="price" min="0" max="10">
        </div>
        <div class="form-group">
          <label class="col-lg-4 control-label">address</label>
          <input  name="address" class="form-control"  type="date">
        </div>
        <div class="form-group">
          <label class="col-lg-4 control-label">Open Time</label>
          <div class="col-lg-8 inputGroupContainer">
          <input  name="open" placeholder="Open Time" class="form-control"  type="number" min="0" max="23">
          </div>
        </div>
        <div class="form-group">
          <label class="col-lg-4 control-label">Close Time</label>
          <div class="col-lg-8 inputGroupContainer">
          <input  name="close" placeholder="Close Time" class="form-control"  type="number" min="0" max="23">
          </div>
        </div>
      </div>
</body>
