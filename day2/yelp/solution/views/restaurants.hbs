<h3>Contacts</h3>
<table class="table table-hover">
  <thead>
    <td><b>name</b></td>
    <td><b>price</b></td>
    <td><b>category</b></td>
    <td><b>openTime</b></td>
    <td><b>closingTime</b></td>
    <td><b>Links</b></td>
  </thead>
  <tbody>
    {{#each restaurants}}
    <tr>
      <td>{{name}}</td>
      <td>{{price}}</td>
      <td>{{category}}</td>
      <td>{{openTime}}</td>
      <td>{{closingTime}}</td>
      <td>
        <a href="/restaurants/{{id}}">Restaurant Page</a>
      </td>
    </tr>
    {{#if review}}
    <tr>
      <td>{{../content}}</td>
    </tr>
    {{/if}}
    {{/each}}
  </tbody>
</table>
    <a href="/restaurants/new">New Restaurant</a>
