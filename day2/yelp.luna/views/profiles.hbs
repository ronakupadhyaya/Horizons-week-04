<h4>User Directory</h4>

{{#each users}}
<div class="panel panel-default" style="width: 20em;">
  <div class="panel-heading">
    <h3 class="panel-title">{{this.displayName}}</h3>
  </div>
  <div class="panel-body">
    <p><strong>Location:</strong> {{this.location}}</p>
    <p><a href="/user/{{this._id}}">View private page</a></p>
  </div>
</div>
{{/each}}
